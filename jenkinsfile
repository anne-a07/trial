pipeline {
    agent any
    
    stages {
        stage('Build') {
            steps {
                sh 'docker build -t anne07/flask .'
            }
        }
        stage('Login and Push') {
            steps {
                script {
                    withDockerRegistry(credentialsID: 'Docker') {
                        docker.build('anne07/flask').push('latest')
                    }
                }
            }
        }
    }
}